{
  "name": "@julusian/jpeg-turbo",
  "version": "2.1.0",
  "description": "Limited libjpeg-turbo bindings for Node.js.",
  "keywords": [
    "jpeg",
    "jpeg-turbo",
    "libjpeg-turbo"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/julusian/node-jpeg-turbo.git"
  },
  "binary": {
    "napi_versions": [
      7
    ]
  },
  "engines": {
    "node": ">=14.15"
  },
  "main": "./index.js",
  "types": "./module.d.ts",
  "dependencies": {
    "cmake-js": "^7.0.0",
    "node-addon-api": "^5.0.0",
    "pkg-prebuilds": "~0.1.0"
  },
  "scripts": {
    "install": "pkg-prebuilds-verify ./binding-options.js || cmake-js compile --target jpeg-turbo",
    "rebuild": "cmake-js rebuild --target jpeg-turbo",
    "test": "jest"
  },
  "devDependencies": {
    "jest": "^29.3.1"
  }
}
